{
  "name": "aemaddin/laravel-zoho-api-wrapper",
  "description": "This is ZOHO REST API-V2 wrapper",
  "require": {
    "php": "^7.0",
    "guzzlehttp/guzzle": "^6.3",
    "ext-json": "*"
  },
  "require-dev": {
    "ext-curl": "*",
    "laravel/framework": "5.8.*",
    "laravel/tinker": "^1.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Amr Ahmed",
      "email": "aemaddin@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Aemaddin\\Zoho\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aemaddin\\Zoho\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aemaddin\\Zoho\\ZohoServiceProvider"
      ]
    }
  }
}
